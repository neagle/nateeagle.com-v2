<?xml version="1.0"?>
feed(xmlns='http://www.w3.org/2005/Atom')
	title Nate Eagle - Posts and Bits (old feed)
	link(href='http://nateeagle.com/')
	link(type='application/atom+xml', rel='self', href='http://nateeagle.com/atom.xml')
	updated=moment().format('ddd, DD MMM YYYY HH:mm:ss z')
	id http://nateeagle.com/
	author
		name Nate Eagle
		email nate@nateeagle.com

		for post in posts
			entry
				id=`http://nateeagle.com/${post.filename}`
				link(type='text/html', rel='alternate', href=`http://nateeagle.com/${post.filename}`)
				title=post.title
				updated=post.date
				author
					name Nate Eagle
					uri http://nateeagle.com/
				content(type='html')=post.__content
